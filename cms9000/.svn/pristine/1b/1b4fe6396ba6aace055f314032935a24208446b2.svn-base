package com.teamway.cms.model;

public class ResultMessageModel {
	

	 public ResultMessageModel() {
			this.error = 0;
			this.msg = null;
	 }
	
	 public ResultMessageModel(int error) {
			this.error = error;
			this.msg = null;
	 }
	
	 public ResultMessageModel(int error, String msg) {
		this.error = error;
		this.msg = msg;
	}
	private int  error;
	private String  msg;
	public int getError() {
		return error;
	}
	public void setError(int error) {
		this.error = error;
	}
	public String getMsg() {
		return msg;
	}
	public void setMsg(String msg) {
		this.msg = msg;
	}
}
