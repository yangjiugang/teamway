package com.teamway.cms.pgserver;

public class VideoPojo {
	public int iVideoId; 
	public int iVideoFlag; 
	public int iTranType; 
	public byte[] szDstIpAddr = new byte[32];
	public int iDstPort; 
	public int iVideoCodec;
	public byte[] mark = new byte[1024];
	
	public byte[] getMark() {
		return mark;
	}
	public void setMark(byte[] mark) {
		System.arraycopy(mark, 0, this.mark, 0, mark.length);
	}
	public int getiVideoId() {
		return iVideoId;
	}
	public void setiVideoId(int iVideoId) {
		this.iVideoId = iVideoId;
	}
	public int getiVideoFlag() {
		return iVideoFlag;
	}
	public void setiVideoFlag(int iVideoFlag) {
		this.iVideoFlag = iVideoFlag;
	}
	public int getiTranType() {
		return iTranType;
	}
	public void setiTranType(int iTranType) {
		this.iTranType = iTranType;
	}
	public byte[] getSzDstIpAddr() {
		return szDstIpAddr;
	}
	public void setSzDstIpAddr(byte[] szDstIpAddr) {
		System.arraycopy(szDstIpAddr, 0, this.szDstIpAddr, 0, szDstIpAddr.length);
		
	}
	public int getiDstPort() {
		return iDstPort;
	}
	public void setiDstPort(int iDstPort) {
		this.iDstPort = iDstPort;
	}
	public int getiVideoCodec() {
		return iVideoCodec;
	}
	public void setiVideoCodec(int iVideoCodec) {
		this.iVideoCodec = iVideoCodec;
	} 

    
}
