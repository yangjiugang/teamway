package com.teamway.cms.pgserver;

public class VideoClientPojo {
	String srcAddress; //ip + port
	String client_udp_ip;
	int client_udp_port;


	public VideoClientPojo() {
	}


	public VideoClientPojo(String srcAddress, String client_udp_ip, int client_udp_port) {
		this.srcAddress = srcAddress;
		this.client_udp_ip = client_udp_ip;
		this.client_udp_port = client_udp_port;
	}


	public String getSrcAddress() {
		return srcAddress;
	}


	public void setSrcAddress(String srcAddress) {
		this.srcAddress = srcAddress;
	}


	public String getClient_udp_ip() {
		return client_udp_ip;
	}


	public void setClient_udp_ip(String client_udp_ip) {
		this.client_udp_ip = client_udp_ip;
	}


	public int getClient_udp_port() {
		return client_udp_port;
	}


	public void setClient_udp_port(int client_udp_port) {
		this.client_udp_port = client_udp_port;
	}


}
